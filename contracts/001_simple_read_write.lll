{
    ;; Writes an init flag to transient storage then reads from it and returns the value.

	(def "T_INIT_ADDR" 0x00)
	(def "T_COUNTER_ADDR" 0x20)

	(def "tInit" (TLOAD (ADDRESS) T_INIT_ADDR))

	(def "tInitW" (val) (TSTORE T_INIT_ADDR val))

	(def "StaticInit"
	    (unless tInit {
		(tInitW 1)
	    })
	)

	StaticInit

	(return tInit)
}
